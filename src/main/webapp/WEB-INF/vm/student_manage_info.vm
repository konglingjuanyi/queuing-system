				                  <th>性别</th>
				                  <th>学校</th>
				                  <th>专业</th>
				                  <th>学历</th>
				                  <th>电话</th>
				                  <th>面试时间</th>
				                  <th>面试职位</th>
				                  <th>面试状态</th>
				                  <th>操作</th>
				                </tr>
				              </thead>
				              <tbody>
				              	#foreach($in in $list)
				                <tr class="success">
				                  <td>$in.obj.name</td>
				                  <td>$in.obj.sex</td>
				                  <td>$in.obj.school</td>
				                  <td>$in.obj.profession</td>
				                  <td>$in.obj.education</td>
				                  <td>$in.obj.phone</td>
				                  <td>$in.additionalInfos.get(0)</td>
				                  <td>$in.obj.job</td>
				                  <td>$in.obj.state</td>	              
				                  <td>
				                  	<a href="#">查看</a>
				                  	<a href="#">删除</a>
				                  </td>	
				                </tr>
				                 #end
				              </tbody>
				            </table>
