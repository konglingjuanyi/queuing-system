#set($layout = "layout/default.vm")
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<title>Qunar 日常报销系统</title>
<base href="/oaengine/"></base>
</head>
<body bgcolor="#FFFFFF">
	<script
		src="https://qsso.corp.qunar.com/lib/qsso-auth.js?t=1">
	</script>
	<div align="center" style="margin-top:8%">
	<form method="post" action="oa/login">
	<table style="text-align: center;">
		<tr>
			<td colspan="2" height="100px">
					<img src="images/logo.png" border="0" alt="" />
			</td>
		</tr>
		<tr>
			<td height="40px"
				style="font-size: 23px; text-align: center; font-weight: bolder; letter-spacing: 2px;" >Qunar 日常报销系统</td>
		</tr>
		<tr>
			<td style="font-size: 15px; text-align: center;">
				$message
			</td>
		</tr>
		<tr>
			<td style="font-size: 15px; text-align: center;">
				用户名:<input type="text" name="userName">
			</td>
		</tr>
		<tr>
			<td style="font-size: 15px; text-align: center;">
				密&nbsp;&nbsp;码:<input type="password" name="password">
			</td>
		</tr>
		<tr>
			<td style="font-size: 15px; text-align: center;">
				<input type="submit" value="登录">
			</td>
		</tr>
	</table>
	</form>
	</div>
	<script>
		QSSO.attach('qsso-login', '$request.getContextPath()/oa/login');
	</script>
</body>
</html>


