								  <input type="password" id="password" placeholder="必填">
								</div>
							</div>
							<div class="control-group">
								<label class="control-label" for="project">面试项目</label>
								<div class="controls">
									<table class="table table-bordered">
						              <thead>
						                <tr>
						                  <th></th>
						                  <th>初试</th>
						                  <th>复试</th>
						                  <th>一次决定权</th>
						                </tr>
						              </thead>
